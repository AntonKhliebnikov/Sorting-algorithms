        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-49" value="Шаг 2.&amp;nbsp;Определение количества блоков и создание структуры блоков" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;" vertex="1" parent="1">
