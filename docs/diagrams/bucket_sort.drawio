        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-29" value="Исходный массив int[] array" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;" vertex="1" parent="1">
          <mxGeometry x="-414" y="325" width="240" height="60" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-30" value="int min = array[0];&lt;br&gt;int max = array[0];" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;" vertex="1" parent="1">
          <mxGeometry x="-414" y="415" width="240" height="60" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-31" value="Для каждого value из array:&#xa;если value меньше min → min = value&#xa;если value больше max → max = value" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1f5e1;strokeColor=#82b366;" vertex="1" parent="1">
          <mxGeometry x="-414" y="495" width="240" height="90" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-32" value="int n = array.length;&lt;br&gt;int bucketCount = n / 3;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;" vertex="1" parent="1">
          <mxGeometry x="-414" y="595" width="240" height="70" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-33" value="Создаем List&amp;lt;List&amp;lt;Integer&amp;gt; buckets:" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;" vertex="1" parent="1">
          <mxGeometry x="-414" y="685" width="240" height="80" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-34" value="range = max - min + 1" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;" vertex="1" parent="1">
          <mxGeometry x="-80" y="325" width="200" height="50" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-35" value="Распределяем по блокам:&lt;br&gt;для каждого value из array:&lt;br&gt;  bucketIndex = (value - min) * bucketCount / range;&lt;br&gt;  если bucketIndex == bucketCount, то bucketIndex = bucketCount - 1;&lt;br&gt;  добавляем value в buckets[bucketIndex]" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1f5e1;strokeColor=#82b366;" vertex="1" parent="1">
          <mxGeometry x="-80" y="400" width="380" height="140" as="geometry" />
        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-36" value="Для каждого bucket из buckets:&lt;br&gt;вызываем Collections.sort(bucket)" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;" vertex="1" parent="1">
