        </mxCell>
        <mxCell id="XKg_pHJs-Gt9ILL_SLqK-37" value="Сборка результата:&#xa;index = 0;&#xa;для каждого bucket из buckets:&#xa;  для каждого value в bucket:&#xa;    array[index] = value;&#xa;    index++" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#e1f5e1;strokeColor=#82b366;" vertex="1" parent="1">
